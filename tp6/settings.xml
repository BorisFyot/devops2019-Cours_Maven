<settings>
  <!-- the path to the local repository - defaults to ~/.m2/repository
  -->
  <!-- <localRepository>/path/to/local/repo</localRepository> -->
  <activeProfiles>
    <!--make the profile active all the time -->
    <activeProfile>artifact</activeProfile>
  </activeProfiles>
  <profiles>
    <profile>
      <id>nexus</id>
      <!--Override the repository (and pluginRepository) "central" from the Maven Super POM
          to activate snapshots for both! -->
	<mirrors>
    		<mirror> 
      			<id>nexus</id>
      			<mirrorOf>*</mirrorOf>
      			<url>http://localhost:8081/repository/maven-central/</url>
    		</mirror>
  	</mirrors>
    </profile>
   <profile>
      <id>artifact</id>
      <!--Override the repository (and pluginRepository) "central" from the Maven Super POM
          to activate snapshots for both! -->
	<mirrors>
		<mirror> 
      			<id>artifactory</id>
      			<mirrorOf>*</mirrorOf>
      			<url>http://localhost:8081/artifactory/jcenter/</url>
    		</mirror>
  	</mirrors>
    </profile>
     <profile>
      <id>ProjetA</id>
	<mirrors>
